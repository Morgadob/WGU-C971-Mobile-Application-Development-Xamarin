<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Contacts.TermDetails">
    <ContentPage.Content>
        <StackLayout Margin="20,20,20,0">



            <Label Text="Term Name:" FontAttributes="Bold" TextColor="Black"/>
            <Label Text="Term Name"  x:Name="termDetailName" TextColor="Black"/>
            <BoxView HeightRequest="1" Color="LightGray"/>
            <Label Text="Start Date:" FontAttributes="Bold" TextColor="Black"/>
            <Label Text="startdate" x:Name="termDetailStart"/>
            <Label Text="End Date" FontAttributes="Bold" TextColor="Black"/>
            <Label Text="endDate" x:Name="termDetailEnd"/>



        </StackLayout>
    </ContentPage.Content>
</ContentPage>